    "watch": "./node_modules/watchify/bin/cmd.js ./example/index.js -o ./example/main.js --debug;./node_modules/watchify/bin/cmd.js ./example/default/index.js -o ./example/default/main.js --debug;./node_modules/watchify/bin/cmd.js ./test/test.js -o ./test/main.js --debug;./node_modules/watchify/bin/cmd.js ./example/scrollable-x/index.js -o ./example/scrollable-x/main.js --debug;./node_modules/watchify/bin/cmd.js ./example/scrollable-y/index.js -o ./example/scrollable-y/main.js --debug;",
    "test": "npm run build; grunt"
  },
  "dependencies": {
    "q": "~0.9.x"
  },
